{
  "version": "3.0.0",
  "license": "MIT",
  "author": {
    "name": "Sebastian Zilles",
    "email": "sebastian@zilles.io",
    "url": "https://zilles.io"
  },
  "keywords": [
    "opendash",
    "dashboard",
    "visualization",
    "react"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.openinc.de"
  },
  "files": [
    "assets/*",
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "npm-run-all -s build:*",
    "build:clean": "rimraf ./dist",
    "build:bundle": "tsc",
    "prep:prettier": "prettier --write \"src/**/*\"",
    "prep:exports": "node ./scripts/exports.js"
  },
  "dependencies": {
    "@opendash/i18n": "nightly",
    "@opendash/icons": "nightly",
    "@opendash/ui": "nightly",
    "async-validator": "^3.4.0",
    "highcharts": "^7.0.0",
    "immer": "^5.0.0",
    "lodash.throttle": "^4.0.0",
    "picomatch": "^2.0.0",
    "react-grid-layout": "^0.17.0",
    "react-router": "6.0.0-alpha.5",
    "react-router-dom": "6.0.0-alpha.5",
    "rison": "^0.1.0",
    "simple-statistics": "^7.0.0"
  },
  "peerDependencies": {
    "antd": "^4.0.0",
    "moment": "^2.0.0",
    "parse": "^2.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "antd": "^4.5.4",
    "moment": "^2.27.0",
    "npm-run-all": "^4.1.5",
    "parse": "^2.15.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.0.0",
    "typescript": "^3.9.7",
    "walkdir": "^0.4.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "exports.ts",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "**/translations/**.ts",
        "options": {
          "printWidth": 1000
        }
      }
    ]
  },
  "name": "@opendash/core",
  "description": "open.DASH data visualization framework"
}
