{
  "version": "3.0.0",
  "license": "MIT",
  "author": {
    "name": "Sebastian Zilles",
    "email": "sebastian@zilles.io",
    "url": "https://zilles.io"
  },
  "keywords": [
    "opendash",
    "dashboard",
    "visualization",
    "react"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.openinc.de"
  },
  "files": [
    "assets/*",
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "npm-run-all -s build:*",
    "build:clean": "rimraf ./dist",
    "build:bundle": "tsc",
    "prep:prettier": "prettier --write \"src/**/*\"",
    "prep:exports": "node ./build-tools/exports.js"
  },
  "dependencies": {
    "@ant-design/compatible": "^1.0.0",
    "async-validator": "^3.2.2",
    "highcharts": "^7.2.1",
    "i18next": "^17.0.11",
    "immer": "^5.0.0",
    "lodash.throttle": "^4.1.1",
    "picomatch": "^2.1.1",
    "react-grid-layout": "^0.17.1",
    "react-i18next": "^10.12.1",
    "react-router": "^6.0.0-alpha.1",
    "react-router-dom": "^6.0.0-alpha.1",
    "rison": "^0.1.1",
    "simple-statistics": "^7.1.0"
  },
  "peerDependencies": {
    "@opendash/ui": "^3.0.0-nightly",
    "antd": "^4.0.0",
    "moment": "^2.24.0",
    "parse": "^2.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^3.7.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/react": "^16.9.25",
    "rollup": "^2.10.2",
    "tslib": "^2.0.0",
    "walkdir": "^0.4.1",
    "@opendash/ui": "^3.0.0-nightly",
    "antd": "^4.0.0",
    "moment": "^2.24.0",
    "parse": "^2.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "exports.ts",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "**/translations/**.ts",
        "options": {
          "printWidth": 1000
        }
      }
    ]
  },
  "name": "@opendash/core",
  "description": "open.DASH data visualization framework"
}
