{
  "version": "3.0.0",
  "license": "MIT",
  "author": {
    "name": "Sebastian Zilles",
    "email": "sebastian@zilles.io",
    "url": "https://zilles.io"
  },
  "keywords": [
    "opendash",
    "dashboard",
    "visualization",
    "react"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.openinc.de"
  },
  "files": [
    "assets/*",
    "dist/*"
  ],
  "scripts": {
    "start": "tsc -w",
    "build": "npm-run-all -s build:*",
    "build:clean": "rimraf ./dist",
    "build:bundle": "tsc",
    "prep:prettier": "prettier --write \"src/**/*\"",
    "prep:exports": "node ./scripts/exports.js"
  },
  "dependencies": {
    "@opendash/i18n": "nightly",
    "@opendash/icons": "nightly",
    "@opendash/ui": "nightly",
    "async-validator": "^3.5.1",
    "highcharts": "^9.0.1",
    "immer": "^8.0.1",
    "lodash.throttle": "^4.1.1",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "picomatch": "^2.2.2",
    "react-grid-layout": "^1.2.2",
    "react-router": "6.0.0-alpha.5",
    "react-router-dom": "6.0.0-alpha.5",
    "rison": "^0.1.1",
    "simple-statistics": "^7.6.0"
  },
  "peerDependencies": {
    "antd": "^4.13.0",
    "moment": "^2.29.1",
    "parse": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "antd": "^4.16.9",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "parse": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.2.2",
    "walkdir": "^0.4.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "endOfLine": "lf",
    "overrides": [
      {
        "files": "exports.ts",
        "options": {
          "printWidth": 1000
        }
      },
      {
        "files": "**/translations/**.ts",
        "options": {
          "printWidth": 1000
        }
      }
    ]
  },
  "name": "@opendash/core",
  "description": "open.DASH data visualization framework"
}
